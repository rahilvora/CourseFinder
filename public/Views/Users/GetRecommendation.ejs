<!--new form-->
<script>
    $(function(){
        $("div.subcategory").hide();
        $("input[type=radio]").click(function(){
            var val = $(this).val();
            $(".subcategory").hide();
            $("#subcategory-"+val).show();
        });
    });
</script>
<h3>Please fill in the form to get course recommendation</h3>
<form>
    <div class="form-group">
        <label for="exampleInputEmail1">Main category</label><br>
        <label class="radio-inline"><input type="radio" name="optradio" ng-model="form.category" value="Technology">Technology</label>
        <label class="radio-inline"><input type="radio" name="optradio" ng-model="form.category" value="Business">Business</label>
    </div>
    <div class="form-group subcategory" id="subcategory-Technology">
        <label for="exampleInputEmail1">Subcategory of Technology</label><br>
        <label class="radio"><input type="checkbox" ng-model="form.webdevelopment" name="optradio">Web Development</label>
        <label class="radio"><input type="checkbox" ng-model="form.mobiledevelopment" name="optradio">Mobile Development</label>
        <label class="radio"><input type="checkbox" ng-model="form.bigdata" name="optradio">Big Data</label>
    </div>
    <div class="form-group subcategory" id="subcategory-Business">
        <label for="exampleInputEmail1">Subcategory of Business</label><br>
        <label class="radio"><input type="checkbox" ng-model="form.finance" name="optradio">Finance</label>
        <label class="radio"><input type="checkbox" ng-model="form.accounting"name="optradio">Accounting</label>
    </div>

    <button type="submit" ng-click="submitForm()"class="btn btn-success">Recommendations Please</button>
</form>
